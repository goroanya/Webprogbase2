<!doctype html>
<html lang="en">

<head>
  {{>head}}
  <title>Users</title>
  <meta name="description" content="Users">
  <link rel="stylesheet" type="text/css" href="//netdna.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css">
  <link type="text/css" href="/stylesheets/users.css" rel="stylesheet" />


</head>

{{>menu}}




<hr>
<div class="usersList bootstrap snippet">
  <div class="row">
    <div class="col-lg-12">
      <div class="main-box no-header clearfix">
        <div class="main-box-body clearfix">
          <div class="table-responsive">
            <table class="table user-list">
              <thead>

                <tr>
                  <th style="text-align:center;"><span>User</span></th>
                  <!-- <th style="text-align:center;"><span>Created</span></th> -->
                  <th style="text-align:center;"><span>Role</span></th>
                </tr>

              </thead>
              <tbody>
                {{#users}}
                <tr>
                  <td>
                    <img src={{avaUrl}} alt="avatar">
                    <a href="/users/{{login}}" class="user-link">{{login}}</a>
                    <span class="user-subhead">{{fullname}}</span>
                  </td>
                  <!-- <td><span prettydate>{{registeredAt}}</span></td> -->
                  <td class="text-center">
                    {{#admin}}
                    <span class="label label-danger">{{role}}</span>
                    {{/admin}}
                    {{^admin}}
                    <span class="label label-success">{{role}}</span>
                    {{/admin}}
                  </td>
                </tr>
                {{/users}}
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
























{{>footer}}
</body>

</html>
